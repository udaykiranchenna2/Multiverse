{
    "name": "madeiteasytools/multiverse",
    "description": "Multi-Language Worker Integration for Laravel - Run Python, Node.js and more from your Laravel app",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Udaykiran Chenna",
            "email": "madeiteasytools@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MadeItEasyTools\\Multiverse\\": "src/",
            "MadeItEasyTools\\Multiverse\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadeItEasyTools\\Multiverse\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "symfony/process": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeItEasyTools\\Multiverse\\Providers\\MultiverseServiceProvider"
            ],
            "aliases": {
                "Multiverse": "MadeItEasyTools\\Multiverse\\Facades\\Multiverse"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.9",
        "phpunit/phpunit": "^11.5.3|^12.0"
    },
    "scripts": {
        "test": "../../../vendor/bin/phpunit",
        "test:unit": "../../../vendor/bin/phpunit --testsuite=Unit",
        "test:feature": "../../../vendor/bin/phpunit --testsuite=Feature",
        "test:coverage": "../../../vendor/bin/phpunit --coverage-html coverage"
    }
}
